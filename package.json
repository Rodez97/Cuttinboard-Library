{
  "name": "@cuttinboard-solutions/cuttinboard-library",
  "version": "8.7.6",
  "description": "Shared Code Library for Cuttinboard App",
  "main": "dist/compiled/index.js",
  "types": "dist/compiled/index.d.ts",
  "type": "module",
  "isLibrary": true,
  "files": [
    "dist/compiled/index.js",
    "dist/compiled/index.d.ts",
    "dist/compiled/index.js.map"
  ],
  "scripts": {
    "_clear": "rimraf dist/*",
    "_tsc": "tsc",
    "_make-bundle": "rollup --config",
    "build": "npm run _clear && npm run _tsc && npm run _make-bundle",
    "test": "jest",
    "deploy": "npm run build && npm publish",
    "deploy-root": "npm run build && npm publish src_main/dist",
    "dependency-graph": "dependency-cruiser --exclude '^node_modules' --output-type dot src | dot -Tpng -o dependency-graph.png",
    "generate-docs": "typedoc --entryPointStrategy expand --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "cuttinboard",
    "typescript",
    "interfaces",
    "types",
    "hooks"
  ],
  "author": "Elevvate Technologies, Inc. <info@cuttinboard.com> (https://cuttinboard.com)",
  "license": "Unlicensed",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cuttinboard-Solutions/Cuttinboard-Library.git"
  },
  "bugs": {
    "url": "https://github.com/Cuttinboard-Solutions/Cuttinboard-Library/issues"
  },
  "homepage": "hhttps://github.com/Cuttinboard-Solutions/Cuttinboard-Library",
  "peerDependencies": {
    "@cuttinboard-solutions/types-helpers": ">=1.9.2",
    "date-fns": ">=2.29.3",
    "dayjs": ">=1.11.6",
    "dayjs-recur": ">=1.0.1",
    "firebase": ">=9.20.0",
    "lodash-es": ">=4.17.21",
    "match-sorter": ">=6.3.1",
    "nanoid": ">=4.0.2",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "rxfire": ">=6.0.3",
    "rxjs": ">=7.8.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/date-fns": "^2.6.0",
    "@types/dayjs-recur": "^1.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^12.11.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "rollup": "^3.22.0",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.7",
    "typedoc-plugin-mark-react-functional-components": "^0.2.2",
    "typedoc-plugin-markdown": "^3.15.3",
    "typedoc-plugin-merge-modules": "^5.0.0",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
